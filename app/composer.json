{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Korbeil/DiscordPHP.git"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cweagans/composer-patches": "^1.7",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "pugx/shortid-php": "^1.1",
        "symfony/console": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.0.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.0.*",
        "symfony/yaml": "7.0.*",
        "team-reflex/discord-php": "dev-feature/allow-symfony7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/maker-bundle": "^1.57",
        "symfony/phpunit-bridge": "^7.0",
        "symplify/vendor-patches": "^11.3",
        "zenstruck/foundry": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "cweagans/composer-patches": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Afup\\Hermes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afup\\Hermes\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "patches": {
            "zenstruck/foundry": [
                "patches/zenstruck-foundry-src-proxy-php.patch"
            ]
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    }
}